                                },
                                "example": [
                                    {
                                        "code": "room-join",
                                        "roomId": "cfdd7716-4ed0-4db2-b7d6-93dc0ff72c2a",
                                        "sessionId": "6dfbc23a-4193-4979-adde-e984f40fd80a",
                                        "reticulumId": "1900346259722469400",
                                        "logTime": "2024-11-29T08:54:27.000Z",
                                        "action": "hubs join",
                                        "ip": "0.0.0.0",
                                        "userAgent": "",
                                        "device": "unknown/pc",
                                        "id": 17
                                    }
                                ]
                            }
                        }
                    },
                    "default": {
