    },
    "tags": [
        {
            "name": "authentication",
            "x-displayName": "Authentication",
            "description": "This API uses three types of authentication tokens. The `AccessToken` and `ProjectKey`. \n\n - `AccessToken` can be obtained by first registering an account [(create admin)](#tag/admin/operation/CreateAdmin), followed by logging into that account [(login)](#tag/authentication/operation/Login). \n\n - `ProjectKey` can be generated via 'generate key' after creating a project on the web. \n\n Please ensure that you have the necessary tokens and include them in the header of your requests as required."
        },
        {
            "name": "admin",
            "x-displayName": "Admin",
            "description": "A user in XRCLOUD Developers with full access to the project."
        },
        {
            "name": "project",
            "x-displayName": "Project",
            "description": "*** Currently, only one project can be created per admin account."
        },
        {
            "name": "user",
            "x-displayName": "User",
            "description": "A user in your projects, third-party apps. User registration is required before making any API calls, such as creating or modifying rooms."
        },
        {
            "name": "scene",
            "x-displayName": "Scene"
        },
        {
            "name": "room",
            "x-displayName": "Room",
            "description": "*** Currently, can create 3 rooms per project."
        }
    ],
    "x-tagGroups": [
        {
            "name": "General",
            "tags": [
                "authentication"
            ]
        },
        {
            "name": "API",
            "tags": [
                "admin",
                "project",
                "user",
                "scene",
                "room"
            ]
        }
    ],
    
    "paths": {
        "/auth/login": {
            "post": {
                "tags": [
                    "authentication"
                ],
                "operationId": "Login",
                "summary": "Create an authentication token",
                "description": "Creates and returns a bearer token in JWT format that you can use to \n authenticate with XRCLOUD Developers APIs. \n\n The returned token is used in the HTTP Authorization header like `Authorization: Bearer {AccessToken}`.",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Authentication successful",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginSuccessResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "This can be a 4XX or 5XX error depending on the situation.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
