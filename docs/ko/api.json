    "components": {
        "parameters": {
            "AccessToken": {
                "required": true,
                "name": "Authorization",
                "in": "header",
                "schema": {
                    "type": "string"
                },
                "description": "헤더에 다음과 같은 인증 토큰이 반드시 포함되어야합니다. `Authorization: Bearer {AccessToken}`."
            },
            "ProjectKey": {
                "required": true,
                "name": "Authorization",
                "in": "header",
                "schema": {
                    "type": "string"
                },
                "description": "The header must contain the following `Authorization: Bearer {ProjectKey}`"
            },
            "ProjectId": {
                "required": true,
                "name": "X-Xrcloud-Project-Id",
                "in": "header",
                "schema": {
                    "type": "string"
                },
                "description": "The header must contain the following `X-Xrcloud-Project-Id: {ProjectId}`"
            },
            "PaginationTake": {
                "required": false,
                "name": "take",
                "in": "query",
                "description": "(pagination) number of rows to get.",
                "schema": {
                    "type": "number"
                }
            },
            "PaginationSkip": {
                "required": false,
                "name": "skip",
                "in": "query",
                "description": "(pagination) number of rows to skip.",
                "schema": {
                    "type": "number"
                }
            }
        },
        "schemas": {
            "ErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "description": "HTTP status code. Actual value will depend on the error.",
                        "example": 400
                    },
                    "method": {
                        "type": "string",
                        "description": "HTTP method of the request that caused the error.",
                        "example": "Method"
                    },
                    "url": {
                        "type": "string",
                        "description": "The endpoint that returned this error. This value will vary depending on the endpoint.",
                        "example": "/example/endpoint"
                    },
                    "body": {
                        "type": "object",
                        "description": "The original data sent in the body of the request that caused the error. The actual value will depend on the request.",
                        "example": { "key": "value" }
                    },
                    "message": {
                        "type": "string",
                        "description": "Detailed error message explaining the error.",
                        "example": "Detailed error messages"
                    }
                }
            },
            "CreateAdminRequest": {
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "The email of the XRCLOUD Developers account to authenticate with.",
                        "type": "string",
                        "example": "xrcloud@belivvr.com"
                    },
                    "password": {
                        "description": "The password XRCLOUD Developers account to authenticate with.",
                        "type": "string",
                        "example": "examPassword12!"
                    }
                }
            },
            "CreateAdminSuccessResponse": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "example": "389f0854-231e-4f9e-a0dc-8f801a41fd6a"
                    },
                    "email": {
                        "type": "string",
                        "example": "xrcloud@belivvr.com"
                    }
                }
            },
            "CreateAdminErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "example": "409"
                    },
                    "method": {
                        "type": "string",
                        "example": "POST"
                    },
                    "url": {
                        "type": "string",
                        "example": "/admins"
                    },
                    "body": {
                        "type": "object",
                        "example": {}
                    },
                    "message": {
                        "type": "string",
                        "example": "Admin with email xrcloud@belivvr.com already exists"
                    }
                }
            },
            "LoginRequest": {
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "description": "The email of the XRCLOUD Developers account to authenticate with.",
                        "example": "xrcloud@belivvr.com"
                    },
                    "password": {
                        "type": "string",
                        "description": "The password XRCLOUD Developers account to authenticate with.",
                        "example": "examPassword12!"
                    }
                }
            },
            "LoginSuccessResponse": {
                "type": "object",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.66jdjBjjKZlsiUuNlMZ20W_zl2ADyT49USZiiENwDCw"
                    },
                    "refreshToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.vH8jp9p1RJF5lalQ8dCNatVXFbUfPZQByOs20HUwlFw"
                    }
                }
            },
            "LoginErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "example": "401"
                    },
                    "method": {
                        "type": "string",
                        "example": "POST"
                    },
                    "url": {
                        "type": "string",
                        "example": "/auth/login"
                    },
                    "body": {
                        "type": "object",
                        "example": {}
                    },
                    "message": {
                        "type": "string",
                        "example": "Unauthorized"
                    }
                }
            },
            "RefreshAccessTokenRequest": {
                "type": "object",
                "required": [
                    "refreshToken"
                ],
                "properties": {
                    "refreshToken": {
                        "description": "Token issued with the accessToken at login.",
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.7YlBlmZz5v66QaUEidurmkT254FCvFlbdxSjI9u9u4g"
                    }
                }
            },
            "RefreshAccessTokenSuccessResponse": {
                "type": "object",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.66jdjBjjKZlsiUuNlMZ20W_zl2ADyT49USZiiENwDCw"
                    },
                    "refreshToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.vH8jp9p1RJF5lalQ8dCNatVXFbUfPZQByOs20HUwlFw"
                    }
                }
            },
            "RefreshAccessTokenErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "example": "401"
                    },
                    "method": {
                        "type": "string",
                        "example": "POST"
                    },
                    "url": {
                        "type": "string",
                        "example": "/auth/refresh"
                    },
                    "body": {
                        "type": "object",
                        "example": {}
                    },
                    "message": {
                        "type": "string",
                        "example": "Unauthorized"
                    }
                }
            },
            "CreateUserRequest": {
                "type": "object",
                "required": [
                    "personalId"
                ],
                "properties": {
                    "personalId": {
                        "description": "The app user's unique ID (e.g., UUID).",
                        "type": "string",
                        "example": "7f6ade40-5d48-466c-aea5-93dd05b59aad"
                    }
                }
            },
            "CreateUserSuccessResponse": {
                "type": "object",
                "properties": {
