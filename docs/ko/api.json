    "components": {
        "parameters": {
            "AccessToken": {
                "required": true,
                "name": "Authorization",
                "in": "header",
                "schema": {
                    "type": "string"
                },
                "description": "헤더에 다음과 같은 인증 토큰이 반드시 포함되어야합니다. `Authorization: Bearer {AccessToken}`."
            },
            "ProjectKey": {
                "required": true,
                "name": "Authorization",
                "in": "header",
                "schema": {
                    "type": "string"
                },
                "description": "헤더에 다음과 같은 인증 토큰이 반드시 포함되어야합니다.  `Authorization: Bearer {ProjectKey}`"
            },
            "PaginationTake": {
                "required": false,
                "name": "take",
                "in": "query",
                "description": "(pagination) 가져올 데이터의 갯수.",
                "schema": {
                    "type": "number"
                }
            },
            "PaginationSkip": {
                "required": false,
                "name": "skip",
                "in": "query",
                "description": "(pagination) 건너뛸 데이터의 갯수.",
                "schema": {
                    "type": "number"
                }
            }
        },
        "schemas": {
            "ErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "description": "HTTP 상태 코드. 실제 값은 오류에 따라 달라집니다.",
                        "example": 400
                    },
                    "method": {
                        "type": "string",
                        "description": "오류를 일으킨 요청의 HTTP 메서드입니다.",
                        "example": "Method"
                    },
                    "url": {
                        "type": "string",
                        "description": "이 오류를 반환한 엔드포인트입니다. 이 값은 엔드포인트에 따라 달라집니다.",
                        "example": "/example/endpoint"
                    },
                    "body": {
                        "type": "object",
                        "description": "오류를 일으킨 요청 본문에 전송된 원본 데이터입니다. 실제 값은 요청에 따라 달라집니다.",
                        "example": { "key": "value" }
                    },
                    "message": {
                        "type": "string",
                        "description": "오류를 설명하는 자세한 오류 메시지입니다.",
                        "example": "Detailed error messages"
                    }
                }
            },
            "CreateAdminRequest": {
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "인증할 XRCLOUD Developers 계정의 이메일입니다.",
                        "type": "string",
                        "example": "xrcloud@belivvr.com"
                    },
                    "password": {
                        "description": "인증할 XRCLOUD Developers 계정의 비밀번호입니다.",
                        "type": "string",
                        "example": "examPassword12!"
                    }
                }
            },
            "CreateAdminSuccessResponse": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "example": "389f0854-231e-4f9e-a0dc-8f801a41fd6a"
                    },
                    "email": {
                        "type": "string",
                        "example": "xrcloud@belivvr.com"
                    }
                }
            },
            "CreateAdminErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "example": "409"
                    },
                    "method": {
                        "type": "string",
                        "example": "POST"
                    },
                    "url": {
                        "type": "string",
                        "example": "/admins"
                    },
                    "body": {
                        "type": "object",
                        "example": {}
                    },
                    "message": {
                        "type": "string",
                        "example": "Admin with email xrcloud@belivvr.com already exists"
                    }
                }
            },
            "LoginRequest": {
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "description": "인증할 XRCLOUD Developers 계정의 이메일입니다.",
                        "example": "xrcloud@belivvr.com"
                    },
                    "password": {
                        "type": "string",
                        "description": "인증할 XRCLOUD Developers 계정의 비밀번호입니다.",
                        "example": "examPassword12!"
                    }
                }
            },
            "LoginSuccessResponse": {
                "type": "object",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.66jdjBjjKZlsiUuNlMZ20W_zl2ADyT49USZiiENwDCw"
                    },
                    "refreshToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.vH8jp9p1RJF5lalQ8dCNatVXFbUfPZQByOs20HUwlFw"
                    }
                }
            },
            "LoginErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "example": "401"
                    },
                    "method": {
                        "type": "string",
                        "example": "POST"
                    },
                    "url": {
                        "type": "string",
                        "example": "/auth/login"
                    },
                    "body": {
                        "type": "object",
                        "example": {}
                    },
                    "message": {
                        "type": "string",
                        "example": "Unauthorized"
                    }
                }
            },
            "RefreshAccessTokenRequest": {
                "type": "object",
                "required": [
                    "refreshToken"
                ],
                "properties": {
                    "refreshToken": {
                        "description": "[로그인](#tag/authentication/operation/Login) 시 발급된 토큰입니다.",
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.7YlBlmZz5v66QaUEidurmkT254FCvFlbdxSjI9u9u4g"
                    }
                }
            },
            "RefreshAccessTokenSuccessResponse": {
                "type": "object",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.66jdjBjjKZlsiUuNlMZ20W_zl2ADyT49USZiiENwDCw"
                    },
                    "refreshToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.vH8jp9p1RJF5lalQ8dCNatVXFbUfPZQByOs20HUwlFw"
                    }
                }
            },
            "RefreshAccessTokenErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "example": "401"
                    },
                    "method": {
                        "type": "string",
                        "example": "POST"
                    },
                    "url": {
                        "type": "string",
                        "example": "/auth/refresh"
                    },
                    "body": {
                        "type": "object",
                        "example": {}
