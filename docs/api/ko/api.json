    "components": {
        "parameters": {
            "ApiKey": {
                "required": true,
                "name": "X-XRCLOUD-API-KEY",
                "in": "header",
                "schema": {
                    "type": "string"
                },
                "description": "헤더에 다음과 같은 형태로 인증 키를 포함시켜야합니다. `X-XRCLOUD-API-KEY: ${ApiKey}`."
            },
            "PaginationTake": {
                "required": false,
                "name": "take",
                "in": "query",
                "schema": {
                    "type": "number"
                },
                "description": "(pagination) 가져올 데이터의 갯수."
            },
            "PaginationSkip": {
                "required": false,
                "name": "skip",
                "in": "query",
                "schema": {
                    "type": "number"
                },
                "description": "(pagination) 건너뛸 데이터의 갯수."
            },
            "ProjectId": {
                "required": true,
                "name": "projectId",
                "in": "query",
                "schema": {
                    "type": "uuid"
                },
                "description": "Project의 고유 ID."
            },
            "SceneId": {
                "required": true,
                "name": "sceneId",
                "in": "query",
                "schema": {
                    "type": "uuid"
                },
                "description": "Scene의 고유 ID."
            },
            "RoomProjectId": {
                "required": false,
                "name": "projectId",
                "in": "query",
                "schema": {
                    "type": "uuid"
                },
                "description": "Project의 고유 ID."
            },
            "RoomSceneId": {
                "required": false,
                "name": "sceneId",
                "in": "query",
                "schema": {
                    "type": "uuid"
                },
                "description": "Scene의 고유 ID."
            },
            "Name": {
                "required": false,
                "name": "name",
                "in": "query",
                "schema": {
                    "type": "string"
                },
                "description": "(search) 해당 이름을 가진 Object 조회."
            },
            "Creator": {
                "required": false,
                "name": "creator",
                "in": "query",
                "schema": {
                    "type": "string"
                },
                "description": "(search) 특정 제작자의 Object 조회."
            },
            "SceneCreateCreator": {
                "required": false,
                "name": "creator",
                "in": "query",
                "schema": {
                    "type": "string"
                },
                "description": "Scene의 제작자를 지정할 수 있습니다. 사용자별 'My Assets'를 구분하는 데에 쓰이며 해당 파라미터를 포함하지 않고 호출하여 제작하는 경우 'Admin'이 제작한 것과 동일하게 취급됩니다."
            },
            "SceneCreateCallback": {
                "required": false,
                "name": "callback",
                "in": "query",
                "schema": {
                    "type": "string"
                },
                "description": "Scene 제작 직후에 특정 동작이 필요한 경우(해당 Scene으로 곧장 Room을 제작하는 경우 등) 포함하여 호출합니다. XRCloud 측에서의 호출은 아래와 같이 이루어집니다. \n\n { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify({ sceneId }) }",
                "example": "https://api.xrcloud.app/rooms"
            },
            "UserId": {
                "required": false,
                "name": "userId",
                "in": "query",
                "schema": {
                    "type": "string"
                },
                "description": "Project(app)의 사용자. Room에 입장하는 사용자를 구분하기 위해 사용됩니다."
            },
            "AvatarUrl": {
                "required": false,
                "name": "avatarUrl",
                "in": "query",
                "schema": {
                    "type": "string"
                },
                "description": "Room에서 사용할 아바타 파일의 경로.",
                "example": "https://storage.com/bucket/avatar-file.glb"
            }
        },
        "schemas": {
            "ErrorResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "description": "HTTP 상태 코드. 실제 값은 오류에 따라 달라집니다.",
                        "example": 400
                    },
                    "method": {
                        "type": "string",
                        "description": "오류를 일으킨 요청의 HTTP 메서드입니다.",
                        "example": "Method"
                    },
                    "url": {
                        "type": "string",
                        "description": "이 오류를 반환한 엔드포인트. 이 값은 엔드포인트에 따라 달라집니다.",
                        "example": "/example/endpoint"
                    },
                    "body": {
                        "type": "object",
                        "description": "오류를 일으킨 요청 본문에 전송된 원본 데이터. 실제 값은 요청에 따라 달라집니다.",
                        "example": { "key": "value" }
                    },
                    "message": {
                        "type": "string",
                        "description": "오류를 설명하는 자세한 오류 메시지.",
                        "example": "Detailed error messages"
                    }
                }
            },
            "Project": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "(project_id) Project의 고유 ID.",
                        "example": "f24ee5ba-8f1f-4f7a-86ed-738f95ea1f8b"
                    },
                    "name": {
                        "type": "string",
                        "description": "Project의 이름.",
                        "example": "example project name"
                    },
                    "webhookUrl": {
                        "type": "string",
                        "description": "사용자가 Room에 입장하거나 퇴장할 때의 이벤트 정보를 전송하기 위한 외부 시스템의 엔드포인트 주소. 설정한 URL로 Room 입/퇴장 정보가 POST 요청으로 전송. \n\n { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify({ sceneId }) }",
                        "example": "https://api.xrcloud.app/events/webhook"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Project가 생성된 날짜와 시간.",
                        "example": "2023-07-12T09:27:41.600Z"
                    },
                    "updatedAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Project가 마지막으로 업데이트된 날짜와 시간.",
                        "example": "2023-07-12T09:27:41.600Z"
                    },
                    "faviconUrl": {
                        "type": "string",
                        "description": "Project의 파비콘 URL.",
                        "example": "https://kr.object.ncloudstorage.com/favicon/4a3/4a3e946e-2776-4182-afb7-204d982b727d.ico"
                    },
                    "logoUrl": {
                        "type": "string",
                        "description": "Project의 로고의 URL.",
                        "example": "https://kr.object.ncloudstorage.com/favicon/51d/51dc73cc-36db-4046-af8d-1e61e4b59c19.jpg"
                    }
                }
            },
            "Scene": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "(scene_id) Scene의 고유 ID.",
                        "example": "f24ee5ba-8f1f-4f7a-86ed-738f95ea1f8b"
                    },
                    "name": {
                        "type": "string",
                        "description": "Scene의 이름.",
                        "example": "example scene name"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Scene이 생성된 날짜와 시간.",
                        "example": "2023-07-12T09:27:41.600Z"
                    },
                    "updatedAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Scene이 마지막으로 업데이트된 날짜와 시간.",
                        "example": "2023-07-12T09:27:41.600Z"
                    },
                    "thumbnailUrl": {
                        "type": "string",
                        "description": "Scene의 thumbnail URL.",
                        "example": "https://api.xrcloud.app/files/852a6f47-3978-4201-a637-9699d7845266.jpg"
                    },
                    "sceneModificationUrl": {
                        "type": "string",
                        "description": "Scene을 수정할 공간의 URL.",
                        "example": "https://room.xrcloud.app:4000/spoke/projects/ASJCJK1?optId=35e75895-9db7-4eee-bae3-87e20ee156dc"
                    }
                }
            },
            "Room": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "(room_id) Room의 고유 ID.",
                        "example": "f24ee5ba-8f1f-4f7a-86ed-738f95ea1f8b"
                    },
                    "name": {
                        "type": "string",
                        "description": "Room의 이름.",
                        "example": "example room name"
                    },
                    "size": {
                        "type": "number",
                        "description": "Room에 입장할 수 있는 최대 인원 수.",
                        "example": 10
                    },
                    "tags": {
                        "type": "array",
                        "description": "(Before Use) 특정 Room을 구분하기 위한 필드.",
                        "example": []
                    },
                    "sceneId": {
                        "type": "string",
                        "description": "Scene의 ID(uuid).",
                        "example": "c322b49b-6e12-47d9-b293-b5ab3727459c"
                    },
